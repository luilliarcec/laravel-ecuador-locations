{
    "name": "luilliarcec/laravel-ecuador-locations",
    "description": "Set of provinces, cantons and parishes or cities of Ecuador.",
    "keywords": [
        "laravel",
        "locations",
        "ecuador"
    ],
    "homepage": "https://github.com/luilliarcec/laravel-ecuador-locations",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Andr√©s Arce C.",
            "email": "luilliarcec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Luilliarcec\\EcuadorLocations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luilliarcec\\EcuadorLocations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luilliarcec\\EcuadorLocations\\EcuadorLocationsServiceProvider"
            ]
        }
    }
}
